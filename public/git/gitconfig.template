[color]
  ui = auto
[user]
  name = <%= SETTINGS['name'] %>
  email = <%= SETTINGS['email'] %>
[apply]
  whitespace = nowarn
[alias]
  co = checkout
  st = status
  ci = commit
  sb = show-branch
  up = !git fetch origin && git rebase origin/master
[core]
  excludesfile = <%= PUBLIC_DIR %>/git/gitignore
[branch]
  autosetupmerge = true
[showbranch]
  default = --all
[github]
  user = shenie

